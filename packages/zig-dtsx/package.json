{
  "name": "@stacksjs/zig-dtsx",
  "type": "module",
  "version": "0.9.11",
  "description": "High-performance DTS emitter written in Zig",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
